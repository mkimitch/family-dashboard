{
	"name": "family-dashboard-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "yarn optimize:photos && vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev:lan": "vite dev --host 0.0.0.0 --port 5173",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"optimize:photos": "bash scripts/optimize-photos.sh",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"start": "node build"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^22",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.51.5",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.1.10"
	},
	"packageManager": "yarn@4.11.0+sha512.4e54aeace9141df2f0177c266b05ec50dc044638157dae128c471ba65994ac802122d7ab35bcd9e81641228b7dcf24867d28e750e0bcae8a05277d600008ad54",
	"dependencies": {
		"@lxg/weather-icons": "^3.0.1",
		"lottie-web": "^5.13.0"
	}
}
